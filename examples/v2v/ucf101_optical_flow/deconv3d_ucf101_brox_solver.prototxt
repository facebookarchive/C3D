train_net: "deconv3d_ucf101_brox.prototxt"
# test_net: "conv3d_ucf101_test.prototxt"
test_iter: 0
test_interval: 0
base_lr: 0.0000001
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 200000
# Display every 20 iterations
display: 20
# The maximum number of iterations
max_iter: 1200000
# snapshot intermediate results
snapshot: 10000
snapshot_prefix: "flow_ucf101_brox"
# solver mode: CPU or GPU
solver_mode: GPU
device_id: 2
